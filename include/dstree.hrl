-record(dstree,
        {id,
         status = initial,
         z1 = origin,
         origin,
         z2 = parent,
         parent,
         z3 = children,
         children = ordsets:new(),
         neighbors = ordsets:new(),
         subtree = orddict:new(),
         z7 = dead,
         dead = ordsets:new(),
         tree,
         waiting_timeout,
         waiting_for,
         timers = orddict:new(),
         dbg_fun,
         report_fun,
         send_fun = fun dstree:default_send/2}).
